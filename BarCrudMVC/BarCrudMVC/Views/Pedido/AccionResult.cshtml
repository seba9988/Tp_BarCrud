<body>
	<section>
		<h1 class="text-center">Resultado de la Operacion</h1>
		@if (ViewBag.Fallo != null)
		{
			<article class="alert  alert-danger text-center" role="alert">
				@ViewBag.Fallo
			</article>
		}
		@if (ViewBag.Exito != null)
		{
			<article class="alert alert-success text-center">
				@ViewBag.Exito
			</article>
		}
		<article>

			<a href='javascript:history.go(-1)'><i class="bi bi-arrow-left"></i>Volver</a>
		</article>
	</section>

	
</body>
